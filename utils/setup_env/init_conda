# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/mnt/bn/hjx-nas-arnold/conda/miniforge3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/mnt/bn/hjx-nas-arnold/conda/miniforge3/etc/profile.d/conda.sh" ]; then
        . "/mnt/bn/hjx-nas-arnold/conda/miniforge3/etc/profile.d/conda.sh"
    else
        export PATH="/mnt/bn/hjx-nas-arnold/conda/miniforge3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<